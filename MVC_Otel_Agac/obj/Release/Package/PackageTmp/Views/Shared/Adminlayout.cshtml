
@{

    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="Mosaddek">
    <meta name="keyword" content="FlatLab, Dashboard, Bootstrap, Admin, Template, Theme, Responsive, Fluid, Retina">
    <link rel="shortcut icon" href="img/favicon.png">

    <title>STK - Ticaret -@ViewBag.Title   </title>

    <!-- Bootstrap core CSS -->
    <link href="/Content/Admin/css/bootstrap.min.css" rel="stylesheet">
    <link href="/Content/Admin/css/bootstrap-reset.css" rel="stylesheet">
    <!--external css-->
    <link href="/Content/Admin/assets/font-awesome/css/font-awesome.css" rel="stylesheet" />
    <link href="/Content/Admin/assets/jquery-easy-pie-chart/jquery.easy-pie-chart.css" rel="stylesheet" type="text/css" media="screen" />
    <link rel="stylesheet" href="css/owl.carousel.css" type="text/css">
    <!-- Custom styles for this template -->
    <link href="/Content/Admin/css/style.css" rel="stylesheet">
    <link href="/Content/Admin/css/style-responsive.css" rel="stylesheet" />

    @RenderSection("head", false)
</head>

<body>

    <section id="container">
        <!--header start-->
        <header class="header white-bg">
            <div class="sidebar-toggle-box">
                <div data-original-title="Toggle Navigation" data-placement="right" class="icon-reorder tooltips"></div>
            </div>
            <!--logo start-->
            <a href="/Admin/Index" class="logo">STK<span> Otel Otomasyonu</span></a>
            <!--logo end-->
            <div class="top-nav ">
                <!--search & user info start-->
                <ul class="nav pull-right top-menu">
                    <li>
                        <input type="text" class="form-control search" placeholder="Ara">
                    </li>
                    <!-- user login dropdown start-->
                    <li class="dropdown">
                        <a data-toggle="dropdown" class="dropdown-toggle" href="#">
                            <img alt="" src="/Content/Admin/img/avatar1_small.jpg">
                            <span class="username">Muhammed Ali</span>
                            <b class="caret"></b>
                        </a>
                        <ul class="dropdown-menu extended logout">
                            <!--<div class="log-arrow-up"></div>-->
                            <li><a href="/Admin/CikisYap"><i class="icon-key"></i> Cıkış YAp</a></li>
                        </ul>
                    </li>
                    <!-- user login dropdown end -->
                </ul>
                <!--search & user info end-->
            </div>
        </header>
        <!--header end-->
        <!--sidebar start-->
        <aside>
            <div id="sidebar" class="nav-collapse ">
                <!-- sidebar menu start-->
                <ul class="sidebar-menu" id="nav-accordion">
                    <li>
                        <a class="active" href="/Admin/Index">
                            <i class="icon-dashboard"></i>
                            <span>Ana Sayfa</span>
                        </a>
                    </li>
                    <li>
                        <a class="active" href="/Admin/Oteller">
                            <i class="icon-dashboard"></i>
                            <span>Oteller</span>
                        </a>
                    </li>
                    <li>
                        <a class="active" href="/Admin/Personeller">
                            <i class="icon-dashboard"></i>
                            <span>Personeller</span>
                        </a>
                    </li>
                    <li>
                        <a class="active" href="/Admin/Markalar">
                            <i class="icon-dashboard"></i>
                            <span>Markalar</span>
                        </a>
                    </li>
                    <li>
                        <a class="active" href="/Admin/Musteriler">
                            <i class="icon-dashboard"></i>
                            <span>Musteriler</span>
                        </a>
                    </li>
                    <li>
                        <a class="active" href="/Admin/SliderResimleri">
                            <i class="icon-dashboard"></i>
                            <span>Silder Resimleri</span>
                        </a>
                    </li>
                    <li class="sub-menu">
                        <a href="javascript:;">
                            <i class="icon-laptop"></i>
                            <span>Özellikleri</span>
                        </a>
                        <ul class="sub">
                            <li><a href="/Admin/OzellikTipleri">Özellik Tipleri</a></li>
                            <li><a href="/Admin/OzellikDegerleri">Özellik Degerleri</a></li>
                            <li><a href="/Admin/UrunOzellikleri">Ürün Özellikleri</a></li>
                        </ul>
                    </li>
                    <li class="sub-menu">
                        <a href="javascript:;">
                            <i class="icon-laptop"></i>
                            <span>AlısVerisler</span>
                        </a>
                        <ul class="sub">
                            <li><a href="/Admin/TamamlananSiparisler">Tamamlanan Siparisler</a></li>
                            <li><a href="/Admin/OnayBekleyenSiparisler">Onay Bekleyen Siparisler</a></li>
                            <li><a href="/Admin/KargoyaVerilenSiparisler">Kargoya Verilen Siparisler</a></li>
                        </ul>
                    </li>

                    <li class="sub-menu">
                        <a href="javascript:;">
                            <i class="icon-book"></i>
                            <span>Raporlar</span>
                        </a>
                        <ul class="sub">
                            <li><a href="/Admin/UrunRaporlari">Urun Raporları</a></li>
                            <li><a href="SatisRaporlari">Satis Raporlari</a></li>
                            <li><a href="/Admin/MusteriRaporlari">Musteri Raporlari</a></li>
                        </ul>
                    </li>




                </ul>
                <!-- sidebar menu end-->
            </div>
        </aside>
        <!--sidebar end-->
        <!--main content start-->
        <section id="main-content">
            @RenderBody()
        </section>
        <!--main content end-->
        <!--footer start-->
        <footer class="site-footer">
            <div class="text-center">
                2019 &copy; Powered by STK
                <a href="#" class="go-top">
                    <i class="icon-angle-up"></i>
                </a>
            </div>
        </footer>
        <!--footer end-->
    </section>

    <!-- js placed at the end of the document so the pages load faster -->
    <script src="/Content/Admin/js/jquery.js"></script>
    <script src="/Content/Admin/js/jquery-1.8.3.min.js"></script>
    <script src="/Content/Admin/js/bootstrap.min.js"></script>
    <script class="include" type="text/javascript" src="/Content/Admin/js/jquery.dcjqaccordion.2.7.js"></script>
    <script src="/Content/Admin/js/jquery.scrollTo.min.js"></script>
    <script src="/Content/Admin/js/jquery.nicescroll.js" type="text/javascript"></script>
    <script src="/Content/Admin/js/jquery.sparkline.js" type="text/javascript"></script>
    <script src="/Content/Admin/assets/jquery-easy-pie-chart/jquery.easy-pie-chart.js"></script>
    <script src="/Content/Admin/js/owl.carousel.js"></script>
    <script src="/Content/Admin/js/jquery.customSelect.min.js"></script>
    <script src="/Content/Admin/js/respond.min.js"></script>

    <script class="include" type="text/javascript" src="/Content/Admin/js/jquery.dcjqaccordion.2.7.js"></script>

    <!--common script for all pages-->
    <script src="/Content/Admin/js/common-scripts.js"></script>

    <!--script for this page-->
    <script src="/Content/Admin/js/sparkline-chart.js"></script>
    <script src="/Content/Admin/js/easy-pie-chart.js"></script>
    <script src="/Content/Admin/js/count.js"></script>

    <script>

      //owl carousel

      $(document).ready(function() {
          $("#owl-demo").owlCarousel({
              navigation : true,
              slideSpeed : 300,
              paginationSpeed : 400,
              singleItem : true,
			  autoPlay:true

          });
      });

      //custom select box

      $(function(){
          $('select.styled').customSelect();
      });

    </script>

</body>
</html>



